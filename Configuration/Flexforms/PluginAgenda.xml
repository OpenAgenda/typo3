                    </settings.language>
                    <settings.columns>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:agenda.columns
                        </label>
                        <config>
                            <type>input</type>
                            <default>2</default>
                        </config>
                    </settings.columns>
                    <settings.mapFilterTilesUri>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:agenda.mapFilterTilesUri
                        </label>
                        <config>
                            <type>input</type>
                            <default>https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png</default>
                        </config>
                    </settings.mapFilterTilesUri>
                    <settings.mapFilterTilesAttribution>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:agenda.mapFilterTilesAttribution
                        </label>
                        <config>
                            <type>input</type>
                            <default>OpenStreetMap contributors</default>
                        </config>
                    </settings.mapFilterTilesAttribution>
                </el>
            </ROOT>
        </sDEF>
        <vDEF>
