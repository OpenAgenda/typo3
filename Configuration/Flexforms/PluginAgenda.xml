                    <settings.calendarUid>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:agenda.calendarUid
                        </label>
                        <config>
                            <type>input</type>
                            <eval>required</eval>
                        </config>
                    </settings.calendarUid>
                    <settings.eventsPerPage>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:agenda.eventsPerPage
                        </label>
                        <config>
                            <type>input</type>
                            <default>20</default>
                            <eval>required</eval>
                        </config>
                    </settings.eventsPerPage>
                    <settings.language>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:agenda.language
