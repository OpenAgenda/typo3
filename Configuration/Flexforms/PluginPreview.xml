<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>
                    LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:sheetGeneral
                </sheetTitle>
                <type>array</type>
                <el>
                    <settings.calendarUid>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:agenda.calendarUid
                        </label>
                        <config>
                            <type>input</type>
                        </config>
                    </settings.calendarUid>
                    <settings.eventsInPreview>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:preview.eventsInPreview
                        </label>
                        <config>
                            <type>input</type>
                            <default>20</default>
                        </config>
                    </settings.eventsInPreview>
                    <settings.language>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:preview.language
                        </label>
                        <config>
                            <type>input</type>
                            <default>fr</default>
                        </config>
                    </settings.language>
                    <settings.columns>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:preview.columns
                        </label>
                        <config>
                            <type>input</type>
                            <default>2</default>
                        </config>
                    </settings.columns>
                    <settings.agendaPage>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:preview.agendaPage
                        </label>
                        <config>
                            <type>group</type>
                            <allowed>pages</allowed>
                            <maxitems>1</maxitems>
                            <minitems>1</minitems>
                            <size>1</size>
                            <suggestOptions>
                                <default>
                                    <additionalSearchFields>nav_title, alias, url</additionalSearchFields>
                                    <addWhere>AND pages.doktype = 1</addWhere>
                                </default>
                            </suggestOptions>
                        </config>
                    </settings.agendaPage>
                    <settings.displayAgendaLink>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:preview.displayAgendaLink
                        </label>
                        <config>
                            <type>check</type>
                        </config>
                    </settings.displayAgendaLink>
                    <settings.preFilter>
                        <label>
                            LLL:EXT:openagenda/Resources/Private/Language/locallang_db.xlf:agenda.preFilter
                        </label>
                        <config>
                            <type>input</type>
                        </config>
                    </settings.preFilter>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>